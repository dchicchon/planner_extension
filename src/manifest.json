{
  "manifest_version": 2,
  "name": "Polus",
  "version": "1.7.2",
  "description": "An online planner to help make you more efficient!",
  "icons": {
    "16": "assets/polus_tab_icon.png",
    "48": "assets/polus_icon48.png",
    "128": "assets/polus_icon128.png"
  },
  "browser_action": {
    "default_title": "Polus",
    "default_icon": "assets/polus_tab_icon.png",
    "default_popup": "html/popup.html"
  },
  "author": "dchicchon",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "html/index.html"
  },
  "key": "",
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
  "incognito": "not_allowed",
  "offline_enabled": true,
  "optional_permissions": [
    "notifications"
  ],
  "oauth2": {
    "client_id": "274422794820-42usjoqhn5sm4cfvngusc2089erhr8mh.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "permissions": [
    "alarms",
    "contextMenus",
    "identity",
    "storage"
  ],
  "short_name": "Polus"
}